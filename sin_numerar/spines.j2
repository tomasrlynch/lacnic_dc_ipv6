interface eth1
no ipv6 nd ra-interval 5
ipv6 nd suppress-ra
interface eth2
no ipv6 nd ra-interval 5
ipv6 nd suppress-ra
interface eth3
no ipv6 nd ra-interval 5
ipv6 nd suppress-ra
interface eth4
no ipv6 nd ra-interval 5
ipv6 nd suppress-ra
router bgp {{ bgp.as }}
{% for ngb in bgp.neighbors if 'spine' in ngb.name %}
no neighbor {{ ngb.ipv6 }}
{% endfor %}
